    "jest-resolve": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.5",
    "ts-patch": "^2.1.0",
    "tscpaths": "^0.0.9",
